import numpy as np
import matplotlib.pyplot as plt
import random


Land = 0
Total = 0

for y in range(0, 10000000):
    Place = 1
    Total = Total + 1
    for z in range(0, 25):
        x = random.randint(1,2)
        Place = Place + x
        if Place == 25:
            Land = Land + 1
            

prob = Land/Total
print(Land)
print(Total)
print(prob)
